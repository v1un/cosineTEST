generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Produto {
  id          Int      @id @default(autoincrement())
  nome        String
  descricao   String
  preco       Float
  imagemUrl   String
  criadoEm    DateTime @default(now())
  atualizadoEm DateTime @updatedAt
}