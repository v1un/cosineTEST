generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Produto {
  id          Int      @id @default(autoincrement())
  nome        String
  descricao   String
  preco       Float
  imagemUrl   String
  criadoEm    DateTime @default(now())
  atualizadoEm DateTime @updatedAt
}